package eventStorage

type EventBuilder interface () {

}

type EventBuilderImpl struct {
	EventName      string
	EventStartTime string
	EventEndTime string
	EventLocation  string
	EventOrganiser int64
	EventType      string
	Theme          string
	InvitedMembers string
	IntrestedCount int64
	GoingCount     int64
	ReservedSeats  int64
	EventFee       string
	NotGoingCount  int64
	IsDeleted      bool
	IsActive       bool
	CreatedBy      int64
	UpdatedBy      time.Time
}
